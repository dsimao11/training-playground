pipeline {
	agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
	parameters {
        	string(name: 'NAME', defaultValue: 'defaultName', description: 'What is the name?')
		string(name: 'AGE', defaultValue: 'defaultName', description: 'What is the name?')
    	}
	stages {
        	stage('build') {
            		steps {
                		echo "$NAME"
                		echo "$AGE"
            		}	
       		 }
    	}
}
