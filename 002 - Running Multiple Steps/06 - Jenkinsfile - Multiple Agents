pipeline {
    agent none
    stages {
    	stage('buildMaven') {
		agent {
			docker {
				image 'mvn:3.3.3'
			}
		}
		steps {
			sh "mvn --version"
		}
	}
	   stage('buildNpm') {
	   	agent {
			docker {
				image 'node:6.3'
			}
		}
		steps {
			sh "npm --version"
		}
	}
    }
}
