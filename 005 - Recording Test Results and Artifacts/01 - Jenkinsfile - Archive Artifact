pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
    stages {
        stage('build') {
            steps {
                sh "curl www.google.com > googleCurl.html"
            }
        }
    }
        post {
        always {
            archiveArtifacts artifacts: 'googleCurl.html'
        }
    }
}
